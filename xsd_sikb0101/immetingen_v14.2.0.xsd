	<element name="Biota" type="immetingen:BiotaType" substitutionGroup="gml:AbstractObject">
		<annotation>
			<documentation>Speciaal datatype voor uitbreidingen aan het monster voor de biota kenmerken voor bio-monsters.</documentation>
		</annotation>
	</element>
	<complexType name="BiotaType">
		<sequence>
			<element name="organism" type="gml:CodeType" minOccurs="0">
				<annotation>
					<documentation>organisme - Nadere aanduiding van het organisme waarin het monster is genomen.
Domeintabel: Biotaxon</documentation>
				</annotation>
			</element>
			<element name="organ" type="gml:CodeType" minOccurs="0">
				<annotation>
					<documentation>Soort orgaan.
Domeintabel: Orgaan</documentation>
				</annotation>
			</element>
			<element name="sampleCriterium" type="gml:CodeType" minOccurs="0" maxOccurs="unbounded">
