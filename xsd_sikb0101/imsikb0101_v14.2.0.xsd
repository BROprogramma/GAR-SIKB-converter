								<reversePropertyName xmlns="http://www.opengis.net/gml/3.2">imsikb0101:siteOfInterest</reversePropertyName>
							</appinfo>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
